<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="pkan.dcatapde">
<body>
    <metal:content-core fill-slot="content-core">

      <div tal:condition="view/base_object_url">
        <h2 i18n:translate="">Base Object</h2>
        <a tal:attributes="href view/base_object_url"
           tal:content="view/base_object_title"></a>
      </div>
      <h2 i18n:translate="">Field Mapping</h2>
      <div tal:repeat="ct view/data">
        <h3 tal:content="ct/ct"></h3>
        <table>
            <thead>
                <tr>
                    <td i18n:translate="">Dcat Field</td>
                  <td i18n:translate="">Source Field</td>
                  <td i18n:translate="">Prio</td>
                </tr>
            </thead>
            <tbody>
                <tr tal:repeat="line ct/lines">
                    <td tal:content="line/dcat_field"></td>
                    <td tal:content="line/source_field"></td>
                    <td tal:content="line/prio"></td>
                </tr>
            </tbody>
        </table>
      </div>
    </metal:content-core>
</body>
</html>
