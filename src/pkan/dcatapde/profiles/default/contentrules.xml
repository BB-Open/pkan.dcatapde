<?xml version="1.0"?>
<contentrules>
  <rule
      cascading="False"
      description=""
      enabled="True"
      event="zope.lifecycleevent.interfaces.IObjectAddedEvent"
      name="pkan_dcatapde_move_dct_licensedocument_to_folder"
      stop-after="True"
      title="Move License Documents to License Folder">
    <conditions>
      <condition type="plone.conditions.PortalType">
        <property name="check_types">
          <element>dct_licensedocument</element>
        </property>
      </condition>
    </conditions>
    <actions>
      <action type="plone.actions.Move">
        <property name="target_folder">/licenses</property>
      </action>
    </actions>
  </rule>
  <assignment
      bubbles="True"
      enabled="True"
      location=""
      name="pkan_dcatapde_move_dct_licensedocument_to_folder"
      />
</contentrules>
